{
  "version": 2,
  "alias": "set.visnup.com",
  "builds": [
    { "src": "/api/*/index.js", "use": "@now/node" },
    { "src": "/next/next.config.js", "use": "@now/next" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/next/$1" }
  ],
  "env": {
    "DB_URL": "@cheat-at-set-debug-db-url"
  }
}
